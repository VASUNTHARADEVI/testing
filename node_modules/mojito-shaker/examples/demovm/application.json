[
    {
        "settings": [
            "master"
        ],
        "mojitsDirs": [
            "mojits",
            "node_modules/mojito-shaker/mojits"
        ],
        "staticHandling": {
            "useRollups": false
        },
        "specs": {
            "htmlframe": {
                "type": "ShakerHTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "title": "Shaker Demo",
                    "child": {
                        "type": "master"
                    }
                }
            }
        },
        "assets": {
            "/static/assets/regional-fonts.css": {
                "path": "",
                "type": "css"
            }
        }
    },
    {
        "settings": [
            "environment:test"
        ],
        "shaker": {
            "task": "local"
        }
    },
    {
        "settings": [
            "environment:stage"
        ],
        "shaker": {
            "task": "s3",
            "images": true,
            "parallel": 8,
            "delay": 0,
            "config": {
                "client": {
                    "key": "<key>",
                    "secret": "<secret>",
                    "bucket": "<bucket>"
                }
            }
        }
    },
    {
        "settings": [
            "asset_mode:prod"
        ],
        "assets": {
            "/static/assets/mycommon/utils.css": {
                "path": "/mycommon/utils.css",
                "type": "css"
            },
            "/static/autoload/compiled/shaker-meta.common.js": {
                "path": "/a/compiled/shaker-meta.common.js",
                "type": "js"
            }
        }
    },
    {
        "settings": [
            "asset_mode:dev"
        ],
        "assets": {
            "/static/assets/mycommon/utils.css": {
                "path": "/static/assets/mycommon/utils.css",
                "type": "css"
            },
            "/static/autoload/compiled/shaker-meta.common.js": {
                "path": "/static/autoload/compiled/shaker-meta.common.js",
                "type": "js"
            }
        }
    },
    {
        "settings": [
            "lang:ru-RU"
        ],
        "assets": {
            "/static/assets/regional-fonts.css": {
                "path": "/vm-ru.css",
                "type": "css"
            }
        }
    },
    {
        "settings": [
            "asset_mode:dev",
            "lang:ru-RU"
        ],
        "assets": {
            "/static/assets/regional-fonts.css": {
                "path": "/static/assets/vm-ru.css",
                "type": "css"
            }
        }
    }
]